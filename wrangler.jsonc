{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "minimalist-django",
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist/",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "compatibility_date": "2025-05-05",
  "routes": [
    {
      "pattern": "minimalistdjango.com",
      "custom_domain": true
    }
  ]
}
